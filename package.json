    "helmet": "^5.0.2",
    "morgan": "^1.10.0",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chance": "^1.1.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
