{
  "name": "game-session-analysis",
  "version": "1.0.0",
  "description": "Project for game session analysis",
  "main": "dist/index.js",
  "scripts": {
    "build:docker": "docker build -t game-sessions-mysql ./src/schema",
    "build:ts": "tsc -b tsconfig.json",
    "build": "npm run build:docker && npm run build:ts",
    "start:db": "docker run -p 3306:3306 --name game-sessions-mysql game-sessions-mysql",
    "start:server": "tsc -w & nodemon dist/index.js",
    "debug:db": "docker container exec -it game-sessions-mysql bash",
    "pretest": "npm run lint",
    "test": "jest",
    "lint": "eslint --fix --ext .ts ."
  },
  "author": "gilisho@github.com",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@types/chance": "^1.1.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
    "@types/mysql": "^2.15.21",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-eslint": "^10.1.0",
    "chance": "^1.1.8",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)+(spec|test|it).ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "testEnvironment": "node"
  }
}
